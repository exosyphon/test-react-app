{
  "extends": [
    "prettier",
    "react-app",
    "react-app/jest"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "semi": false }],
    "semi": ["error", "never"]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true
  }
}
